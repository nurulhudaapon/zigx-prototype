pub fn Page(ctx: zx.PageContext) zx.Component {
    ctx.response.header("Location", "/doc");
    ctx.response.setStatus(.temporary_redirect);

    return (
        <span @allocator={ctx.arena}>
            Redirecting to /doc...
        </span>
    );
}

const std = @import("std");
const zx = @import("zx");
