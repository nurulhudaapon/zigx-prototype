pub fn Layout(allocator: zx.Allocator, children: zx.Component) zx.Component {
  return (
    <html lang="en-US">
      <head>
        <title>ZigX</title>
      </head>

      <body>
        <template shadowrootmode="open">
          {(children)}
        </template>
      </body>
    </html>
  );
}

const zx = @import("zx");
