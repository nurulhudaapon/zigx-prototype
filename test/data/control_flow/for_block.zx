pub fn Page(allocator: zx.Allocator) zx.Component {
    const chars = "ABC";
    return (
        <main @allocator={allocator}>
            {for (chars) |char| (
                <div>
                    <i>{[char:c]}</i>
                </div>
            )}
        </main>
    );
}

const zx = @import("zx");
